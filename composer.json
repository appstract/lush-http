{
    "name": "appstract/lush-http",
    "description": "Smart Http Client for PHP.",
    "keywords": [
        "php",
        "appstract",
        "lush-http",
        "lush",
        "curl",
        "http",
        "client"
    ],
    "homepage": "https://github.com/appstract/lush-http",
    "license": "MIT",
    "authors": [
        {
            "name": "Olav van Schie",
            "email": "hello@appstract.team",
            "homepage": "https://appstract.team",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/support": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Appstract\\LushHttp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Appstract\\LushHttp\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Lush": "Appstract\\LushHttp\\LushFacade"
            }
        }
    }
}
